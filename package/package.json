  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "^9.1.12",
    "@storybook/react-vite": "^9.1.12",
    "@svgr/rollup": "^8.1.0",
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.52.4",
    "storybook": "^9.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.5.0"
  }
}
