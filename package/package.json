{
  "name": "@holmesdev/cursors",
  "version": "0.0.0",
  "description": "React component for custom cursors - SVG based, fully-customizable, and accessibility-focused",
  "author": "Haus of Cards",
  "keywords": [
    "react",
    "cursor",
    "custom cursor",
    "animated cursor",
    "ui",
    "react-component",
    "svg"
  ],
  "license": "MIT",
  "homepage": "https://github.com/haus-of-cards/cursors",
  "repository": {
    "type": "git",
    "url": "https://github.com/haus-of-cards/cursors.git"
  },
  "bugs": {
    "url": "https://github.com/haus-of-cards/cursors/issues"
  },
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
